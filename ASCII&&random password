                        Console.WriteLine("                                  ----                                                         ----");
                        Console.WriteLine("     Barış AKIN                                                                                PüsSamet AKKUŞ    ");
                        Console.WriteLine("\n");
                        Console.Write(DateTime.Now);                         
                            if (c == 59)
                        {
                            b++;
                            if (b == 60)
                            {
                                b = 0;
                                a++;
                                if (a == 24)
                                {
                                    a = 0;
                                }
                            }
                        }
                    }
                 }
            }
