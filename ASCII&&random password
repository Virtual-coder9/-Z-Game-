                Console.Read();
                int a = 0, b = 0, c;
                while (true)
                {
                    for (c = 0; c < 60; c++)
                    {
                        System.Threading.Thread.Sleep(1000);
                        Console.Clear();
                        Console.ForegroundColor = ConsoleColor.Red;
                        Console.WriteLine("                                       ___________   _______________                 ________                   ");
                        Console.WriteLine("                                ----    |  ____  |   |_______________|               /      |   ----");
